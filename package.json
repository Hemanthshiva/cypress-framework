{
  "name": "cypress-framework",
  "version": "1.0.0",
  "description": "Cypres Automation Framework",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run:on:chrome": "cypress run --headed --browser chrome",
    "cy:run:on:edge": "cypress run --headed --browser edge",
    "cy:run:headless": "cypress run",
    "cy:run:with:test:tag": "cypress run --spec cypress/e2e/cucumber-tests/featureFiles/*.feature --headed --browser chrome --env TAGS=@test",
    "cy:report": "node ./cypress/report.ts",
    "cy:parallel": "cypress-parallel -s cy:run -t 3 -d cypress/e2e/cucumber-tests/featureFiles/*.feature"
  },
  "author": "Hemanth Shivashankarappa",
  "license": "ISC",
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "@types/cypress-cucumber-preprocessor": "^4.0.5",
    "@types/node": "^20.12.12",
    "@types/resolve": "^1.20.6",
    "cypress": "^13.13.3",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-xpath": "^2.0.1",
    "multiple-cucumber-html-reporter": "^3.6.2"
  },
  "dependencies": {
    "@cucumber/gherkin-utils": "^9.0.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-parallel": "^0.14.0",
    "npx": "^3.0.0",
    "tsify": "^5.0.4",
    "typescript": "^5.4.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/e2e/cucumber-tests",
    "cucumberJson": {
      "generate": true
    }
  }
}
